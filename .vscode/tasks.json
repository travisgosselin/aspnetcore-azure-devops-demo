{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish"
            ],
            "options": {
                "cwd": "${workspaceFolder}/src/Demo.Web/"
            },
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run"
            ],
            "options": {
                "cwd": "${workspaceFolder}/src/Demo.Web/"
            },
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "run",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run"
            ],
            "options": {
                "cwd": "${workspaceFolder}/src/Demo.Web/"
            },
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "unit test",
            "command": "dotnet",
            "type": "process",
            "args": [
                "test"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "docker build",
            "command": "docker",
            "type": "shell",
            "args": [
                "build",
                "-t",
                "my-meetup-demo-app:latest",
                "."
            ],
            "group": "build",
            "problemMatcher": []
        },
        {
            "label": "docker launch",
            "command": "docker",
            "type": "shell",
            "args": [
                "run",
                "--name",
                "my-meetup-demo-app",
                "-d",
                "-p",
                "8080:80",
                "my-meetup-demo-app:latest"
            ],
            "group": "build",
            "problemMatcher": []
        }
    ]
}