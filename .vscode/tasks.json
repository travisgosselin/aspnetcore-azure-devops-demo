{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish"
            ],
            "options": {
                "cwd": "${workspaceFolder}/src/Demo.Web/"
            },
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run"
            ],
            "options": {
                "cwd": "${workspaceFolder}/src/Demo.Web/"
            },
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "run",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run"
            ],
            "options": {
                "cwd": "${workspaceFolder}/src/Demo.Web/"
            },
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "unit test",
            "command": "dotnet",
            "type": "process",
            "args": [
                "test"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        }
    ]
}